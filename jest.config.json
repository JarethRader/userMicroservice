{
  "testRegex": "((\\.|/*.)(spec))\\.(ts|js)?$",
  "transform": {
    "^.+\\.(ts|tsx)$": "ts-jest"
  },
  "coveragePathIgnorePatterns": ["/node_modules/"],
  "moduleDirectories": ["node_modules", "__test__"],
  "moduleFileExtensions": ["ts", "js"],
  "globals": {
    "ts-jest": {
      "tsconfig": "tsconfig.json"
    }
  },
  "preset": "@shelf/jest-mongodb"
}
